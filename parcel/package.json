{
  "name": "setup-vue-without-vue-cli",
  "description": "How to create skeleton project in Vue.js from basic without Vue-CLI",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Irfan Maulana (https://github.com/mazipan/)",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/parcel index.html",
    "build": "rimraf dist && npm run lint && cross-env NODE_ENV=production ./node_modules/.bin/parcel build index.html",
    "lint": "./node_modules/.bin/eslint --ext .js,.vue src --fix --no-ignore"
  },
  "homepage": "https://github.com/mazipan/setup-vue-without-vue-cli/",
  "repository": {
    "type": "git",
    "url": "https://github.com/mazipan/setup-vue-without-vue-cli.git"
  },
  "bugs": {
    "url": "https://github.com/mazipan/setup-vue-without-vue-cli/issues"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-hot-reload-api": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@vue/component-compiler-utils": "2.3.0",
    "babel-core": "7.0.0-bridge.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "es6-promise": "^4.2.5",
    "eslint": "^5.7.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-vue": "4.7.1",
    "node-sass": "^4.9.4",
    "parcel-bundler": "1.10.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  }
}
